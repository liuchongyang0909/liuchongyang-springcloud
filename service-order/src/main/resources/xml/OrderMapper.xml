<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.liuchongyang.mapper.OrderMapper">
    <select id="getOrderById" resultType="com.liuchongyang.Order">
        SELECT * FROM t_order WHERE id=#{id}
    </select>
    
    <select id="findAll" resultType="com.liuchongyang.Order">
        SELECT * FROM t_order
    </select>
</mapper>